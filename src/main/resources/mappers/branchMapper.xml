<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.jinw0909.portfolio.commons.branch.dao.BranchDAO">
 	<select id="selectBranchById" resultType="com.jinw0909.portfolio.commons.branch.model.Branch">
 		SELECT
 			*
 		FROM
 			`branch`
 		WHERE
 			`id` = #{id}
 	</select>
 </mapper>